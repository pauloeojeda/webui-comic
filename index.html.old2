    <!-- Login -->
    <script id="loginForm" type="text/template">
      <form class="form-signin" role="form">
        <h2 class="form-signin-heading">Please sign in</h2>
        <input type="email" class="form-control" placeholder="Email address" required autofocus>
        <input type="password" class="form-control" placeholder="Password" required>
        <label class="checkbox">
          <input type="checkbox" value="remember-me"> Remember me
        </label>
        <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
      </form>
    </script>

    <!-- Comic Element -->

    <script id="comicElement" type="text/template">
      <a href="#<%= link %>">
          <img src="<%= cover %>" alt="<%= name %>" class="image"/>
          <ul>
              <li class="comicInfo"><strong>Id Comic:</strong> <%= idComic %></li>
              <li class="comicInfo"><strong>Id Genre:</strong> <%= idGenre %></li>
              <li class="comicInfo"><strong>Name:</strong> <%= name %></li>
              <li class="comicInfo"><strong>Number:</strong> <%= number %></li>
              <li class="comicInfo"><strong>Cover:</strong> <%= cover %></li>
          </ul>
      </a>
    </script>
    
